<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Sylius\Bundle\GridBundle\Migration\CodeGenerator">
            <argument type="service" id="Sylius\Bundle\GridBundle\Migration\CodeOutputter" />
        </service>

        <service id="Sylius\Bundle\GridBundle\Migration\CodeOutputter" />

        <service id="Sylius\Bundle\GridBundle\Migration\ConfigMigration">
            <argument type="service" id"Sylius\Bundle\GridBundle\Migration\CodeGenerator" />
            <argument type="service" id"Sylius\Bundle\GridBundle\Migration\GridBodyGenerator" />
            <argument type="service" id="container" />
        </service>

        <service id="Sylius\Bundle\GridBundle\Migration\GridBodyGenerator">
            <argument type="service" id="Sylius\Bundle\GridBundle\Migration\ActionMethodGenerator">
            <argument type="service" id="Sylius\Bundle\GridBundle\Migration\CodeGenerator" />
        </service>

        <service id="Sylius\Bundle\GridBundle\Migration\ActionMethodGenerator">
            <argument type="service" id="Sylius\Bundle\GridBundle\Migration\CodeGenerator" />
        </service>
    </service>
</services>
